<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mapeditor.controller.MapEditorAddContinentController">
   <children>
      <Text fx:id="txt_addContinentTitle" layoutX="102.0" layoutY="131.0" strokeType="OUTSIDE" strokeWidth="0.0" text="AddContinent">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Text fx:id="txt_continentName" layoutX="355.0" layoutY="322.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Continent name">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <Text fx:id="txt_continentBonus" layoutX="355.0" layoutY="466.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Continent bonus">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <TextField fx:id="txf_continentName" layoutX="652.0" layoutY="299.0" onKeyPressed="#detectInputValidation" onKeyReleased="#detectInputValidation" promptText="Asia" />
      <TextField fx:id="txf_continentBonus" layoutX="652.0" layoutY="443.0" onKeyPressed="#detectInputValidation" onKeyReleased="#detectInputValidation" promptText="7" />
      <Button fx:id="btn_addContinentApply" layoutX="398.0" layoutY="620.0" mnemonicParsing="false" onAction="#clickToAddContinent" text="Apply" />
      <Button fx:id="btn_ok" layoutX="719.0" layoutY="620.0" mnemonicParsing="false" onAction="#clickToOk" text="OK" />
   </children>
</AnchorPane>
